    <!-- Add Tiny Slider JS to your body -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.3/min/tiny-slider.js"></script>
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Initialize Tiny Slider without controls
            var popularSlider = tns({
                container: '#popularSlider',
                items: 1,
                slideBy: 'page',
                autoplay: true,
                controlsText: ["", ""], // Set controlsText to an empty string to hide navigation buttons
            });
        });
